<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/MaterialDesignColor.Blue.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="TabHeaderHighlightBrush" Color="#DEDEDE" po:Freeze="True" />
    <SolidColorBrush x:Key="TabHeaderInactiveBrush" Color="Gray" po:Freeze="True" />

    <SolidColorBrush x:Key="TextForeground" Color="#DEDEDE" po:Freeze="True" />
    <SolidColorBrush x:Key="SelectedTextForeground" po:Freeze="True" Color="#1E1E1E" />
    <SolidColorBrush x:Key="SelectedItemBackground" Color="LightGray" po:Freeze="True" />
    <SolidColorBrush x:Key="MouseOverItemTextForeground" Color="#1E1E1E" po:Freeze="True" />
    <SolidColorBrush x:Key="MouseOverItemBackground" Color="Gray" po:Freeze="True" />
    <SolidColorBrush x:Key="MenuHeaderForeground" Color="WhiteSmoke" po:Freeze="True" />

    <SolidColorBrush x:Key="MultiSelectComboBox.BorderBrush" Color="White" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBox.MouseOver.BorderBrush" Color="WhiteSmoke" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBox.Background" Color="{StaticResource Primary600}" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBox.Foreground" Color="#1E1E1E" po:Freeze="True" />

    <SolidColorBrush x:Key="MultiSelectComboBox.ToggleButton.MouseOver.Color" Color="White" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBox.ToggleButton.Pressed.Color" Color="White" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBox.Selection.Color" Color="Red" po:Freeze="True" />

    <SolidColorBrush x:Key="MultiSelectComboBoxItem.Background" Color="WhiteSmoke" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBoxItem.Foreground" Color="#1E1E1E" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBoxItem.Indicator.Color" Color="Transparent" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBoxItem.Indicator.Selected.Color" Color="{StaticResource Primary500}" po:Freeze="True" />
    <SolidColorBrush x:Key="MultiSelectComboBoxItem.Border.Selected.Color" Color="LightGray" po:Freeze="True" />

</ResourceDictionary>