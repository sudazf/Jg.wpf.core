<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <SolidColorBrush x:Key="TabHeaderHighlightBrush" Color="#DEDEDE" po:Freeze="True" />
    <SolidColorBrush x:Key="TabHeaderInactiveBrush" Color="Gray" po:Freeze="True" />

    <SolidColorBrush x:Key="TextForeground" Color="#DEDEDE" po:Freeze="True" />
    <SolidColorBrush x:Key="TextSelectedForeground" po:Freeze="True" Color="#1E1E1E" />
    <SolidColorBrush x:Key="ItemSelectedBackground" Color="Gray" po:Freeze="True" />
    <SolidColorBrush x:Key="ItemMouseOverBackground" Color="LightGray" po:Freeze="True" />

</ResourceDictionary>